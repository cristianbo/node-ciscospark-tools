{
  "name": "ciscospark-tools",
  "version": "0.1.0",
  "author": "Tor E Hagemann <tor.hagemann@gmail.com>",
  "description": "Helps you grok Cisco Spark",
  "homepage": "https://github.com/hagemt/node-ciscospark-tools#readme",
  "license": "MIT",
  "main": "support/index.js",
  "bugs": {
    "url": "https://github.com/hagemt/node-ciscospark-tools/issues"
  },
  "dependencies": {
    "commander": "2.13.0",
    "debug": "3.1.0",
    "inquirer": "5.0.1",
    "lodash": "4.17.5",
    "node-fetch": "2.0.0",
    "uuid": "3.2.1",
    "vorpal": "1.12.0"
  },
  "devDependencies": {
    "eslint": "4.17.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-node": "5.2.1",
    "mocha": "5.0.0",
    "nyc": "11.4.1"
  },
  "directories": {
    "bin": "scripts/bin",
    "doc": "Documentation",
    "example": "scripts",
    "lib": "support",
    "man": "Documentation/man",
    "test": "support/test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "cisco",
    "spark",
    "ciscospark",
    "tool",
    "tools",
    "script",
    "command"
  ],
  "nyc": {
    "exclude": [
      "**/*.tests.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hagemt/node-ciscospark-tools.git"
  },
  "scripts": {
    "coverage": "nyc mocha scripts support; nyc report --reporter=html # and in console",
    "eslint": "eslint .eslintrc.js scripts support --report-unused-disable-directives",
    "postinstall": "npm run script # prints usage hints",
    "postreport": "npm run eslint -- --max-warnings 0",
    "prereport": "npm run eslint -- --fix",
    "report": "npm run coverage",
    "script": "node scripts",
    "start": "npm run script",
    "test": "npm run report"
  }
}
